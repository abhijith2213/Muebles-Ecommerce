<main>

            <!-- breadcrumb-area -->
            <section class="breadcrumb-area breadcrumb-bg" data-background="img/bg/interior-armchair.jpg">
                <div class="container">
                    <div class="row">
                        <div class="col-12">
                            <div class="breadcrumb-content">
                                <h2>Shop Sidebar</h2>
                                <nav aria-label="breadcrumb">
                                    <ol class="breadcrumb">
                                        <li class="breadcrumb-item"><a href="/">Home</a></li>
                                        <li class="breadcrumb-item active" aria-current="page">Shop</li>
                                    </ol>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- breadcrumb-area-end -->

            <!-- shop-area -->
            <section class="shop-area pt-100 pb-100">
                <div class="container">
                    <div class="row">
                        <div class="col-xl-9 col-lg-8" >
                            <div class="shop-top-meta mb-35">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="shop-top-left">
                                            <ul>
                                                <li><a href="#"><i class="flaticon-menu"></i> Products</a></li>
                                                
                                            </ul>
                                        </div>
                                    </div>
                                    {{!-- <div class="col-md-6">
                                        <div class="shop-top-right">
                                            <form action="#">
                                                <select name="select">
                                                    <option value="">Sort by newness</option>
                                                    <option>Free Shipping</option>
                                                    <option>Best Match</option>
                                                    <option>Newest Item</option>
                                                    <option>Size A - Z</option>
                                                </select>
                                            </form>
                                        </div>
                                    </div> --}}
                                </div>
                            </div>
                            <div class="row" id="myDiv">
                                {{#each products}}
                                <div class="col-xl-4 col-sm-6">
                                    <div class="new-arrival-item text-center mb-50">
                                        <div class="thumb mb-25">
                                            {{#if this.offerPercentage}}
                                            <div class="discount-tag new">- {{this.offerPercentage}}%</div>
                                            {{/if}}
                                            <a href="/product-details/{{this._id}}"><img src="/imageUploads/{{this.images.[0]}}" alt="" style="height:344px ; width:296px ;"></a>
                                            <div class="product-overlay-action">
                                                <ul>
                                                    <li><a style="color: white;" id="wishlistbtn" onclick="addToWishlist('{{this._id}}')"><i class="far fa-heart"></i></a></li>
                                                    <li><a href="/product-details/{{this._id}}"><i class="far fa-eye"></i></a></li>
                                                </ul>
                                            </div>
                                        </div>
                                        <div class="content">
                                            <h5><a href="/product-details/{{this._id}}">{{this.Product}}</a></h5>
                                            
                                            {{#if this.orginalPrice}}
                                            <span class="price">₹{{this.price}}</span>
                                            <span class="price text-danger"><del>₹{{this.orginalPrice}}</del></span>
                                            {{else}}
                                                <span class="price">₹{{this.price}}</span>
                                            {{/if}}
                                        </div>
                                    </div>
                                </div>
                                {{/each}}
                             
                            </div>
                            <div class="pagination-wrap">
                                <ul class="nav">
                                    <li class="prev"><a href="">Prev</a></li>
                                    {{#each pageNum}}
                                    <li class="active"><a href="/viewProducts?page={{this}}&&lim=9">{{this}}</a></li>
                                    {{/each}}
                                   
                                    <li class="next"><a href="">Next</a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-xl-3 col-lg-4">
                            <aside class="shop-sidebar">
                                <div class="widget side-search-bar">
                                    <form action="#">
                                        <input type="text">
                                        <button><i class="flaticon-search"></i></button>
                                    </form>
                                </div>
                                <div class="widget">
                                    <h4 class="widget-title">Product Categories</h4>
                                    <div class="shop-cat-list">
                                        {{#each categories}}
                                        <ul>
                                            <li><a href="/categoryproduct/{{this._id}}">{{this.category}}</a></li>
                                            
                                        </ul>
                                        {{/each}}
                                    </div>
                                </div>

              {{!-- PRICE FILTER 

                                <div class="widget">
              <h4 class="widget-title">Price Filter</h4>
              <div class="price_filter">
                <div id="slider-range"  onclick="priceFilter()"></div>
                <div class="price_slider_amount">
                  <span>Price :</span>₹
                  <input
                    type="text"
                   
                    id="minAmount"
                    name="price"
                    value=""
                    placeholder="Add Your Price"
                  />
₹
                    <input
                    type="text"
                   
                    id="maxAmount"
                    name="price"
                    value=""
                    placeholder="Add Your Price"
                  />
                </div>
              </div> 
            </div> --}}
                              
                            </aside>
                        </div>
                    </div>
                </div>
            </section>
            <!-- shop-area-end -->

        </main>

      